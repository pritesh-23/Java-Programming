    }//GEN-LAST:event_jDivideButtonActionPerformed

    private void jAcButtonActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jAcButtonActionPerformed
       jtxtResult.setText("") ;
        String fn , sn ;
        fn = String.valueOf(num1) ;
        sn = String.valueOf(num2) ;
        
        fn = " " ;
        sn = " " ; 
    }//GEN-LAST:event_jAcButtonActionPerformed

    private void jDotButtonActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jDotButtonActionPerformed
        // TODO add your handling code here:
        if(! jtxtResult.getText().contains(".")){
            jtxtResult.setText(jtxtResult.getText() + jDotButton.getText()) ;
        }
    }//GEN-LAST:event_jDotButtonActionPerformed

    private void jEqualButtonActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jEqualButtonActionPerformed
        num2 = Double.parseDouble(jtxtResult.getText()) ;
        if(output == "+"){
            result = num1 + num2 ;
            jtxtResult.setText(String.valueOf(result));
        }
        else if (output == "-"){
              result = num1 - num2 ;
            jtxtResult.setText(String.valueOf(result));
        }
         else if (output == "*"){
              result = num1 * num2 ;
            jtxtResult.setText(String.valueOf(result));
        }
         else if (output == "/"){
              result = num1 / num2 ;
            jtxtResult.setText(String.valueOf(result));
        }
        
    }//GEN-LAST:event_jEqualButtonActionPerformed
    private void EnterNumber(String s){
        String Nums = jtxtResult.getText() + s ;
        jtxtResult.setText(Nums) ;
        
    }
       
